@article{courtelChancellerieActesEudes1977,
  title = {La chancellerie et les actes d'Eudes IV, due de Bourgogne (1315-1349)},
  author = {Courtel, Anne-Lise},
  date = {1977},
  journaltitle = {Bibliothèque de l'École des Chartes},
  volume = {135},
  number = {1},
  pages = {23--71},
  publisher = {{Persée - Portail des revues scientifiques en SHS}},
  doi = {10.3406/bec.1977.450090},
  url = {https://www.persee.fr/doc/bec_0373-6237_1977_num_135_1_450090},
  urldate = {2023-06-06},
  abstract = {Le bureau chargé de la mise par écrit des décisions du duc de Bourgogne, n'est pas encore en 1315, une institution aux contours bien définis. Le chancelier de Bourgogne dont l'office est réapparu après une longue interruption en 1270, n' est que le chef de la juridiction gracieuse de la chancellerie aux contrats et ne porte pas, du fait de son office, le grand sceau ducal, le sceau de la Chambre. La garde de ce sceau est en effet assurée au XIII siècle par le chambellan ou ses subordonnés, et le soin de rédiger les lettres confié des chapelains suivant le duc. Pour le principat d'Eudes IV, aucune ordonnance de l'Hôtel n'a été conservée, qui nous permettrait de connaître précisément la composition et les attributions du personnel chargé des écritures . L'expression «clerc le duc de Bourgoigne» sert à désigner fréquemment, dans les documents, les membres non laïques formant un groupe de spécialistes jurisperiti ou legum professores au sein du Conseil ducal. Parmi ces «clercs et conseillers le duc», un groupe plus restreint encore assure la direction collégiale du «bureau d'écritures»; l' un de ces clercs-conseillers porte le grand sceau de la Chambre, qui n'est plus la garde du chambellan et pas encore, du moins institutionnellement, la garde du chancelier ; un autre clerc-conseiller porte le sceau du secret. Ont été gardes des sceaux d' Eudes IV: Pierre de Semur, Thibaud de Semur, Anseau Peaud'oie, Hugues de Coraboeuf, Jean Aubriot, Hélie Bourgeoise, Jacques d'Audeloncourt. Le personnel subalterne chargé de mettre en forme les actes ducaux est d'origine diverse: outre les clercs et chapelains, des conseillers ecclésiastiques d'E'udes IV et les chapelains suivant le duc, du service de la Chapelle, des clercs sans autre qualificatif sont recrutés parmi les notaires de la chancellerie aux contrats. Une spécialisation des tâches, si souple soit-elle, a dû apparaître vers 1340; peut-être un clerc particulier est-il attaché aux comptes; un clerc dou Consoil est signalé en décembre 1343. Le bureau d'écritures de la duchesse n'est composé que de quelques chapelains et clercs. Les sceaux d'Eudes IV sont au nombre de trois. Le grand sceau de la Chambre symbole par excellence de l'autorité ducale, est de type équestre à l'avers ; le contre-sceau représente l'écu de Bourgogne ancien. En dépit de l'annexion de l'Artois et de la Franche-Comté en 1330, une seule matrice a été utilisée de l'avènement à la mort d'Eudes IV, aucune chancellerie particulière n'ayant été maintenue dans les comtés. Deux sceaux du secret ou petits sceaux ont été en revanche successivement utilisés; ronds, ils sont tous deux de type armorial. La fonte du second sceau du secret - la première empreinte conservée date du 20 octobre 1338 - a été précédée de peu par celle un signet, en août 1338; celui-ci remplace le sceau du secret quand ce dernier est absent ; légèrement plus petit, il représente un buste de profil. La duchesse possède un grand sceau en forme de navette la montrant debout, sous une niche; le contre-sceau de type armorial lui sert également de sceau du secret. De très nombreuses lettres d'Eudes IV ont un petit signet plaqué après la date. Ces signets n'ont pas été apposés par le duc mais par ses conseillers ou officiers qui ont contrôlé ou pris en charge l'acte.},
  langid = {fre},
  keywords = {Chancellerie,histo}
}

